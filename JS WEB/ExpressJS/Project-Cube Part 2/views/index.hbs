<main>
  <h1>Browser</h1>
  <form action="" method="">
    <input type="text" class="search" name="search" placeholder="Search...">
    <input type="number" name="from" class="difficulty" min="1" placeholder="Difficulty from...">
    <span>-</span>
    <input type="number" name="to" class="difficulty" min="1" max="10" placeholder="Difficulty to...">
    <input type="submit" value="search">
  </form>
  {{#each cubes}}
  {{>single-cube}}
  {{/each}}
</main>